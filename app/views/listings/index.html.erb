<p id="notice"><%= notice %></p>
<h1>Listings</h1>
<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th>Spark Listing ID</th>
      <th>Listing api url</th>
      <th colspan="4"></th>
    </tr>
  </thead>

  <tbody>
    <% @listings.each do |listing| %>
      <tr>
        <td><%= listing.spark_id %></td>
        <td><%= listing.listing_api_url %></td>
        <td><%= link_to 'Download', listings_download_path(id:listing.id),method:'POST' %></td>
        <td><%= link_to 'Show', listing %></td>
        <td><%= link_to 'Edit', edit_listing_path(listing) %></td>
        <td><%= link_to 'Destroy', listing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= will_paginate @listings, list_classes: %w(pagination justify-content-end) %>

<br>

<%#= link_to 'New Listing', new_listing_path %>
